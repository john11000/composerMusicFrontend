{
  "name": "qcode-classic-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "check-lint": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\"",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check \"src/**/*.{ts,tsx}\"",
    "prisma:mi": "prisma migrate dev --name",
    "prisma:deploy": "prisma migrate deploy",
    "prepare": "husky install",
    "check-all": "yarn check-format && yarn check-lint && yarn check-types && yarn build"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts,css,md}": "prettier --write"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@material-ui/core": "^4.12.4",
    "@mui/icons-material": "^5.11.11",
    "@mui/lab": "^5.0.0-alpha.123",
    "@mui/material": "^5.11.13",
    "@mui/system": "^5.11.13",
    "@mui/x-date-pickers": "^6.0.1",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.4",
    "base64-js": "^1.5.1",
    "date-fns": "^2.29.3",
    "lamejs": "^1.2.1",
    "mui-datatables": "^4.3.0",
    "mui-file-input": "^2.0.0",
    "next": "13.2.3",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.43.2",
    "react-redux": "^8.0.5",
    "react-to-pdf": "^1.0.1",
    "react-toastify": "^9.1.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "tone": "^14.7.77"
  },
  "devDependencies": {
    "@types/mui-datatables": "^4.3.3",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.3",
    "typescript": "4.9.5"
  }
}
